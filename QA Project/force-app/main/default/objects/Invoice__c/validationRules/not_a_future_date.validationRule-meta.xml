<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>not_a_future_date</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Paid_Date__c &gt; TODAY(),   Submitted_Date__c &gt; TODAY() )</errorConditionFormula>
    <errorMessage>You cant enter a date in the future!</errorMessage>
</ValidationRule>
